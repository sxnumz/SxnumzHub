Wapon = {}
for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
	if v:IsA("Tool") then
	   table.insert(Wapon ,v.Name)
	end
end
for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do  
	if v:IsA("Tool") then
	   table.insert(Wapon, v.Name)
	end
end

Page1:Toggle("Auto Equiped",false,function(vu)
    _G.AutoEquiped = vu
end)

local Weapon = Page1:Drop("Select Weapon",false,Wapon,function(Value)
    SelectToolWeapon = Value
    SelectToolWeaponOld = Value
end)
    
    spawn(function()
         while _G. wait() do
            if _G.AutoEquiped then
                pcall(function()
                game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(SelectToolWeapon))
                end)
            end
        end
    end)

Page1:Button("Refresh Weapon",function()
    Weapon:Clear()
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if v:IsA("Tool") then
            Weapon:Add(v.Name)
        end
    end
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v:IsA("Tool") then
            Weapon:Add(v.Name)
        end
    end
end)



