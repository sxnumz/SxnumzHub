               function(current)
 
                  local fuckulop2 = current.KeyCode == Enum.KeyCode.Unknown and current.UserInputType or current.KeyCode
 
                     if fuckulop2.Name ==  key then
 
                        pcall(callback)
 
                  end
 
               end
 
               )
 
         end
 
